# MrX App Builder Dashboard - Environment Variables
# Copy this file to .env.local and fill in your values

# API Configuration
VITE_API_BASE_URL=https://api.mrx-builder.pages.dev/api

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_DEV_TOOLS=true

# Build Configuration
VITE_APP_NAME=MrX App Builder
VITE_APP_VERSION=1.0.0

# Optional: Custom LLM Endpoint
VITE_CUSTOM_LLM_ENDPOINT=

# Optional: WebSocket URL (for future real-time features)
VITE_WS_URL=wss://ws.mrx-builder.pages.dev

# Development Only
VITE_MOCK_API=true
VITE_LOG_LEVEL=debug